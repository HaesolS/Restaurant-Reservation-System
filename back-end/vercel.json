{
  "version": 2,
  "builds": [
    {
      "src": "./src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/src/app.js",
      "methods": ["GET", "OPTIONS", "POST", "PUT", "DELETE", "PATCH"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS, POST, PUT, DELETE, PATCH",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type"
      }
    },
    {
      "handle": "filesystem"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}